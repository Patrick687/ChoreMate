{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "predev": "npm run codegen",
    "dev": "nodemon --watch src --ext ts,graphql --exec \"tsx src/index.ts\"",
    "debug": "node --inspect ./node_modules/.bin/tsx src/index.ts",
    "debug:test": "cross-env NODE_OPTIONS='--inspect-brk --experimental-vm-modules' NODE_ENV=test jest --runInBand",
    "start": "tsx src/index.ts",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --runInBand",
    "codegen": "graphql-codegen --config ./src/config/codegen.ts",
    "testDbConnection": "tsx src/utils/db/testDbConnection.ts",
    "seedData": "tsx src/data/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
